{
  "rewrites": [
    {
      "source": "/function/:match*",
      "destination": "https://asia-east1-mpwei-logistics-system.cloudfunctions.net/:match*"
    },
    {
      "source": "/api/:match*",
      "destination": "/api/:match*"
    }
  ],
  "headers": [
    {
      "source": "/function/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store"
        }
      ]
    }
  ],
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ]
}
